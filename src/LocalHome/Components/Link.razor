<a class="userlink" href="@Url" target="_blank">
    <span class="userlink-image"><img src="@ImageUrl" alt="@ImageUrl" /></span>
    <span class="userlink-text">@Text</span>
</a>

@code {
    [Parameter]
    public string Text{ get; set; }
    [Parameter]
    public string Url { get; set; }
    [Parameter]
    public string ImageUrl { get; set; }
}
